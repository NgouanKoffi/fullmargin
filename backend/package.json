{
  "name": "fullmargin-backend",
  "version": "1.0.0",
  "private": true,
  "description": "FullMargin backend API",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "test": "cross-env NODE_ENV=test jest --config ./jest.config.cjs --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --config ./jest.config.cjs --watch",
    "test:cov": "cross-env NODE_ENV=test jest --config ./jest.config.cjs --coverage",
    "test:prod": "cross-env NODE_ENV=production jest --config ./jest.config.cjs --runInBand"
  },
  "nodemonConfig": {
    "watch": [
      "server.js",
      "src",
      ".env"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.952.0",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.2.0",
    "form-data": "^4.0.5",
    "geoip-lite": "^1.4.10",
    "google-auth-library": "^10.4.0",
    "googleapis": "^166.0.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "imapflow": "^1.0.196",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.4",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "sharp": "^0.34.5",
    "stripe": "^19.1.0",
    "ua-parser-js": "^2.0.4",
    "wordpress-hash-node": "^1.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.1.4",
    "supertest": "^6.3.4"
  }
}
